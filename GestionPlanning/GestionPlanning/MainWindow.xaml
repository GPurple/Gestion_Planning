<Window x:Class="GestionPlanning.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GestionPlanning"
        mc:Ignorable="d"
        Title="MainWindow" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Padding="0,1,0,0" Width="{DynamicResource {x:Static SystemParameters.FullPrimaryScreenWidthKey}}" Height="{DynamicResource {x:Static SystemParameters.MaximizedPrimaryScreenHeightKey}}" WindowState="Maximized" WindowStartupLocation="CenterScreen">
    <Grid Margin="0,0,2,-21">
        <TextBlock x:Name="textStateTest1" HorizontalAlignment="Left" Margin="1114,92,0,0" TextWrapping="Wrap" Text="Wait test" VerticalAlignment="Top"/>
        <Button x:Name="BtnStartTest1" Content="Start" HorizontalAlignment="Left" Margin="1008,87,0,0" VerticalAlignment="Top" Width="76" Click="StartTest1"/>
        <TextBlock HorizontalAlignment="Left" Height="23" Margin="1008,64,0,0" TextWrapping="Wrap" Text="Test 1: Enregistrement et chargement données" VerticalAlignment="Top" Width="270"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Height="47" Margin="1008,118,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="294"><Run Text="T"/><Run Text="est 2 : Chargement des données csv dans listes puis synchro avec fichier sauvegarde"/></TextBlock>
        <Button x:Name="BtnStartTest2" Content="Start" HorizontalAlignment="Left" Margin="1008,166,0,0" VerticalAlignment="Top" Width="76" Click="StartTest2"/>
        <TextBlock x:Name="textStateTest2" HorizontalAlignment="Left" Margin="1114,166,0,0" TextWrapping="Wrap" VerticalAlignment="Top"><Run Text="Wait test"/></TextBlock>
        <Button x:Name="BtnStartTest3" Content="Start" HorizontalAlignment="Left" Margin="1008,233,0,0" VerticalAlignment="Top" Width="76" Click="StartTest3"/>
        <TextBlock x:Name="textBlock_test3" HorizontalAlignment="Left" Height="21" Margin="1008,200,-112,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="460"><Run Text="T"/><Run Text="est "/><Run Text="3"/><Run Text=" : Chargemen"/><Run Text="t et affichage des listes"/></TextBlock>
        <local:UC_Disp_Controls HorizontalAlignment="Left" Height="144" Margin="31,553,0,0" VerticalAlignment="Top" Width="640"/>
        <local:UC_Display_week x:Name="UC_Disp_Week" HorizontalAlignment="Left" Height="530" Margin="10,10,0,0" VerticalAlignment="Top" Width="950"/>
        <local:UC_Disp_Month x:Name="UC_Disp_Month" HorizontalAlignment="Left" Height="530" Margin="10,10,0,0" VerticalAlignment="Top" Width="950"/>
        <local:UC_display_day x:Name="UC_Disp_Day" HorizontalAlignment="Left" Height="530" Margin="10,10,0,0" VerticalAlignment="Top" Width="950"/>
        <ScrollViewer HorizontalAlignment="Left" Height="300" Margin="1114,387,0,0" VerticalAlignment="Top" Width="190">
            <StackPanel x:Name="STFichesNotPlaced" Height="300" Width="180"/>
        </ScrollViewer>

    </Grid>
</Window>
